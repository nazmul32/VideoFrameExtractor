<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="30"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="30">
            <shape
                android:shape="line"
                android:top="2dp">
                <stroke
                    android:width="2dp"
                    android:color="@color/color_white" />
            </shape>
        </rotate>
    </item>
</layer-list>